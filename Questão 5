#include <stdio.h>

int main() {
    int num1, num2;

    printf("Digite o primeiro valor inteiro: ");
    scanf("%d", &num1);

    printf("Digite o segundo valor inteiro: ");
    scanf("%d", &num2);

    if (num1 > num2) {
        if (num1 % num2 == 0) {
            printf("%d eh maior e eh multiplo de %d\n", num1, num2);
        } else {
            printf("%d eh maior, mas nao eh multiplo de %d\n", num1, num2);
        }
    } else if (num2 > num1) {
        if (num2 % num1 == 0) {
            printf("%d eh maior e eh multiplo de %d\n", num2, num1);
        } else {
            printf("%d eh maior, mas nao eh multiplo de %d\n", num2, num1);
        }
    } else {
        printf("Os dois numeros sao iguais.\n");
    }

    return 0;
}
